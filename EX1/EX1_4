def EX1_4 (number):
    ''' (int)-->day of the week
    this fuction returns day of the week instead number
    EX1_4(1)
    monday
    or
    EX1_4(9)
    Error'''
    if number== 1 :
        return 'monday'
    elif  number== 2:
        return 'tuesday'
    elif  number== 3:
        return 'wednesday'
    elif  number== 4:
        return 'thursday'
    elif number== 5:
        return' friday'
    elif number== 6:
        return 'saturday'
    elif number== 7:
        return 'sunday'
    else:
        return 'Error'
        
print(EX1_4(9))
